
public class TestCondicional {
	
	public static void main(String[] args) {
		System.out.println("Creando condicionales");
		
		int edad = 17;
		int cantidad = 2;
		
		if (edad >= 18) {
			System.out.println("Puedes entrar");
		}
		else {
			if (cantidad >= 2) {
				System.out.println("Eres menor de edad pero puedes ingresar");
			}
			else 
				System.out.println("Lamentablemente no puedes ingresar");
		}
		
	}

}
